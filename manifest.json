{
	"manifest_version" : 2,
	"name" : " Background Color Change",
	"version" : "0.001",
	"content_scripts" : [
		{
			"matches" : [
				"<all_urls>"
             ], "js" : ["content.js"]
            }
        ],
        "background" : {
                      "scripts" : ["background.js"]
        },
        "browser_action" : {
        },
        "icons": {
            "16":"cat-16.png",
            "32":"cat-32.png",
            "48":"cat-48.png"
        },
        "action":{"default_popup":"popup.html"}
    
    }

